<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.springcore.Student" name="student">
		<property name="studentid">
		<value>65165</value>
		</property>
		<property name="studentname">
		<value>Birju prasad</value>
		</property>
		<property name="studentaddress">
		<value>Purvanchal</value>
		</property>
	</bean>


	<bean class="com.springcore.Student" name="student1">
		<!-- This is our bean  -->
		<!-- this all data is metadata -->
		<property name="studentid" value="1651" />
		<property name="studentname" value="Ramprakash" />
		<property name="studentaddress" value="Kali jhaadi k peehce" />
	</bean>


	<bean class="com.springcore.Student" name="student2">
		<!-- This is our bean  -->
		<!-- this all data is metadata -->
		<property name="studentid" value="64848" />
		<property name="studentname" value="Rajnikanth" />
		<property name="studentaddress" value="andheri gufa" />
	</bean>

	<!--This is using p schema-->
	<bean class="com.springcore.Student" name="student3" p:studentid="51654"
		p:studentname="jhaantu prasad" p:studentaddress="lali ki godi" />

</beans>
